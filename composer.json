{
    "name": "phpmvd/phpday-website",
    "description": "Sitio web para la organizacion del phpday.uy",
    "type": "website",
    "require": {
        "silex/silex": "~1.3",
        "symfony/twig-bridge": "~2.7",
        "symfony/translation": "~2.7",
        "symfony/config": "~2.7",
        "symfony/yaml": "~2.7",
        "symfony/form": "~2.7",
        "symfony/validator": "~2.7",
        "symfony/security-csrf": "~2.7",
        "saxulum/saxulum-doctrine-mongodb-provider": "~1.0",
        "guzzlehttp/guzzle": "^5.3",
        "twig/extensions": "^1.2",
        "swiftmailer/swiftmailer": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ismael Ambrosi",
            "email": "isma@phpday.uy"
        }
    ],
    "autoload": {
        "psr-0": { "": "src/" }
     },
    "autoload-dev": {
        "psr-0": { "": "tests/functional" }
     },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "symfony/browser-kit": "~2.6",
        "symfony/css-selector": "~2.6"
    }
}
